{
    "name": "basuke/hpack-php",
    "type": "library",
    "description": "HPACK is a PHP libray for HTTP/2 HPACK header compression RFC7541",
    "keywords": ["http/2","h2","header","compression"],
    "homepage": "https://github.com/basuke/hpack-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Basuke Suzuki",
            "email": "basuke@siesta.co.jp",
            "homepage": "https://basuke.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "HTTP2\\HPACK\\": "src"
        }
    },
    "require-dev": {
        "codeception/codeception": "^3.1.3",
        "codeception/verify": "^0.3.3",
        "symfony/debug": "^3.2",
        "symfony/var-dumper": "^3.2"
    },
    "scripts": {
        "test": [
            "@unittest"
        ],
        "unittest": "codecept run unit",
        "codecept": "codecept",
        "cest": "codecept generate:cest unit",
        "cept": "codecept generate:cept unit"
    }
}
