{
    "name": "basuke/digdug",
    "type": "library",
    "description": "Dig Dug is a PHP libray that helps you quickly pack and unpack",
    "keywords": ["binary","parse","pack","unpack"],
    "homepage": "https://github.com/basuke/DigDug",
    "license": "MIT",
    "authors": [
        {
            "name": "Basuke Suzuki",
            "email": "basuke@siesta.co.jp",
            "homepage": "https://basuke.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "DigDug\\": "src"
        }
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "symfony/debug": "^3.2",
        "symfony/var-dumper": "^3.2"
    },
    "scripts": {
        "test": [
            "@unittest"
        ],
        "unittest": "codecept run unit",
        "codecept": "codecept",
        "cest": "codecept generate:cest unit",
        "cept": "codecept generate:cept unit"
    }
}
